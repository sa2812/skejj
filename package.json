    "conf": "^15.1.0",
    "ink": "^6.7.0",
    "react": "^19.1.0",
    "zod": "^3.0.0"
  },
  "optionalDependencies": {
    "@skejj/engine-darwin-arm64": "0.1.0",
    "@skejj/engine-darwin-x64": "0.1.0",
    "@skejj/engine-linux-arm64": "0.1.0",
    "@skejj/engine-linux-x64": "0.1.0",
