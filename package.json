{
  "name": "skejj",
  "version": "0.1.0",
  "description": "Constraint-based schedule solver â€” Rust core, Node.js CLI",
  "type": "module",
  "bin": {
    "skejj": "./dist/index.js"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "build:rust": "cargo build --release --manifest-path crates/engine/Cargo.toml",
    "build:all": "npm run build:rust && npm run build",
    "prepublishOnly": "npm run build",
    "start": "tsx src/index.ts",
